#include<conio.h>
#include<iostream>
class largest
{	public:
	int n[5],max;
	void get()
	{
		std::cout<<"Enter 5 nums\n";
		for(int c=0; c<5; c++)
		{	std::cin>>n[c];
			if(c==0)
			{
				max=n[0];
			}	
			else
			{	
				if(n[c]>max)
				{
					max=n[c];	
				}
					
			}
		}
		std::cout<<"Largest="<<max;
	}
};
int main()
{	largest l;
	l.get();
}
